{
  "name": "gcp-miniappkit-jssdk",
  "version": "0.4.0",
  "description": "JS-SDK for Toyota Wallet miniapps",
  "author": "asmyshlyaev177",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/opn-ooo/gcp-miniappkit-jssdk.git"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "types": "types/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "commit": "git-cz",
    "build": "run-s build:types types:cleanup build:bundle",
    "build:bundle": "yarn build:bundle:basic --format umd,modern",
    "build:bundle:basic": "microbundle-crl --compress --external none --strict --sourcemap false",
    "build:types": "rm -rf types && tsc --build tsconfig.json",
    "build:release": "run-s build:types types:cleanup build:bundle build:package",
    "build:package": "cp ./package.json ./dist/package.json",
    "types:cleanup": "cd types && ls -A | grep -v index | grep -v sdk | xargs rm -rf",
    "changelog": "conventional-changelog -p gcp-miniappkit-jssdk -i CHANGELOG.md -s -r 0",
    "get-version": "conventional-recommended-bump -p angular",
    "start": "microbundle-crl watch --no-compress --external none --strict --format umd,modern",
    "serve-static": "http-server dist --cors --no-dotfiles -p 3333 -d",
    "start:miniapp": "cd ./examples/miniapp && yarn install && yarn start",
    "start:example:react": "cd ./src/__tests__/example-react && yarn install && yarn start",
    "start:example:nextjs": "cd ./src/__tests__/example-nextjs && yarn install && yarn build && yarn start",
    "start:example:nextjs:dev": "cd ./src/__tests__/example-nextjs && yarn install && yarn dev",
    "start:example:typescript": "cd ./src/__tests__/example-typescript && yarn install && yarn start",
    "wait-devserver": "wait-on http://127.0.0.1:3000",
    "cypress:open": "cypress open",
    "cypress:run": "yarn wait-devserver && cypress run",
    "test:e2e:all": "run-s test:e2e:react test:e2e:nextjs test:e2e:typescript",
    "test:e2e:react": "start-server-and-test start:example:react 3000 cypress:run",
    "test:e2e:nextjs": "start-server-and-test start:example:nextjs 3000 cypress:run",
    "test:e2e:typescript": "start-server-and-test start:example:typescript 3000 cypress:run",
    "lint": "eslint --fix src --resolve-plugins-relative-to .",
    "prepare": "run-s build",
    "test": "jest",
    "test:ci": "yarn test --all --ci",
    "test:all": "run-s lint test:ci build",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "jest -o && lint-staged && yarn build && git add types",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "last 30 Android version",
    "chrome >= 49",
    "ChromeAndroid >= 49",
    "last 3 safari version",
    "ios_saf >= 11",
    "not IE <= 11",
    "not IE_Mob <= 11",
    "not op_mini all"
  ],
  "resolutions": {
    "browserslist": "^4.16.5",
    "node-notifier": "^8.0.1",
    "handlebars": "^4.7.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/parser": "^7.12.11",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "commitizen": "^4.2.3",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-recommended-bump": "^6.1.0",
    "core-js": "3",
    "cross-env": "^7.0.2",
    "cypress": "6.3.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-no-wildcard-postmessage": "^0.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-unicorn": "^23.0.0",
    "http-server": "^0.12.3",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "semantic-release": "^17.4.2",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.12.1",
    "wait-on": "^5.3.0"
  },
  "files": [
    "dist",
    "types"
  ],
  "dependencies": {}
}
